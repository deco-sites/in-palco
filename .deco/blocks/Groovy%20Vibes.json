{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "2rem",
    "--btn-focus-scale": "0.95",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "4rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "mainColors": {
    "base-100": "#f2f2f2",
    "primary": "#b02a59",
    "secondary": "#d84773",
    "tertiary": "#d85c36",
    "neutral": "#3d3d3d",
    "info": "#5b5b8e",
    "error": "#d85c36",
    "warning": "#d85c36",
    "success": "#008060"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "700"
          }
        ],
        "family": "Quicksand"
      }
    ]
  },
  "mode": "light"
}